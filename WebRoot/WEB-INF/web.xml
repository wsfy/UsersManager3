<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.hsp.view.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginClServlet</servlet-name>
    <servlet-class>com.hsp.controller.LoginClServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MainFrame</servlet-name>
    <servlet-class>com.hsp.view.MainFrame</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManageUsers</servlet-name>
    <servlet-class>com.hsp.view.ManageUsers</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserClServlet</servlet-name>
    <servlet-class>com.hsp.controller.UserClServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Ok</servlet-name>
    <servlet-class>com.hsp.view.Ok</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Err</servlet-name>
    <servlet-class>com.hsp.view.Err</servlet-class>
  </servlet>





  
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginClServlet</servlet-name>
    <url-pattern>/LoginClServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MainFrame</servlet-name>
    <url-pattern>/MainFrame</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManageUsers</servlet-name>
    <url-pattern>/ManageUsers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserClServlet</servlet-name>
    <url-pattern>/UserClServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Ok</servlet-name>
    <url-pattern>/Ok</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Err</servlet-name>
    <url-pattern>/Err</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
